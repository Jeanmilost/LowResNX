DO
    MUSIC (0)

    CALL MAINLOOP
LOOP

SUB MAINLOOP

    REPEAT
        TEXT 2, 0, "C64"
        SPRITE 0 PAL 0 SIZE 1
        SPRITE 0, 20, 10, 5

        TEXT 10, 0, "SPECTRUM"
        SPRITE 1 PAL 0 SIZE 1
        SPRITE 1, 105, 10, 33

        TEXT 0, 4, "SP. COL."
        SPRITE 2 PAL 0 SIZE 1
        SPRITE 2, 20, 42, 37

        TEXT 10, 4, "SP. COL. 2"
        SPRITE 3 PAL 0 SIZE 1
        SPRITE 3, 105, 42, 1

        TEXT 0, 8, "CUSTOM"
        SPRITE 4 PAL 0 SIZE 1
        SPRITE 4, 20, 75, 35

        TEXT 10, 8, "CUSTOM 2"
        SPRITE 5 PAL 0 SIZE 1
        SPRITE 5, 105, 75, 3

        WAIT VBL
    UNTIL 0
END SUB

